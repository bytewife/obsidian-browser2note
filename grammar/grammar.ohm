Links {
	Start = Recurse
    Recurse = (MainLink | MainBacklink | text)*
    MainLink = (~exclude text)* link Recurse
    MainBacklink =  (~exclude text)* backlink Recurse
    exclude = space | link | backlink
   	text = ~exclude any
    link = "[" (~"]" text)* "]" "(" (~")" text)* ")"
    backlink = "[[" (~"]]" text)* "]]"
}